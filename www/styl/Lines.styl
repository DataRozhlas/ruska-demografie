&.lines
  .line
    h2
      font-size 18px
      font-weight 400
    svg
      path
        fill none
        stroke RussianBlue
        stroke-width 1.5px
      circle
        stroke RussianBlue
        fill white
        stroke-width 1px
        &.significant
          fill RussianBlue
      g.axis
        line.extent
        line.mark
          stroke #d
          shape-rendering crispEdges
          &.significant
            stroke RussianBlue
        text
          font-size 11px
          fill darken(RussianBlue, 50%)


